<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/head.ejs") %>
<body>
    <%- include("./partials/nav.ejs") %>
    <h1>Booklist Index</h1>
    <p>Click title to see more.</p>
    <% for (let i=0; i<books.length; i++) { %>
        <div>
            <a href="books/<%= books[i]._id %>">
                <%= books[i].title %>
            </a>
        </div>
    <% } %>
</body>
</html>